    "date-fns": "^2.29.3",
    "keycloak-js": "^20.0.1",
    "lodash": "^4.17.21",
    "nprogress": "^0.2.0",
    "vue": "2.7.16",
    "vue-router": "^3.6.5",
    "vuetify": "^2.6.12",
    "vuex": "^3.6.2",
    "vuex-map-fields": "^1.4.1",
    "vuex-persistedstate": "^4.1.0",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.5.4",
    "@vue/cli-plugin-babel": "^5.0.8",
    "@vue/cli-plugin-eslint": "^5.0.8",
    "@vue/cli-plugin-router": "^5.0.8",
