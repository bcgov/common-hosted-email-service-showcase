    "@vue/test-utils": "^1.3.6",
    "@vue/vue2-jest": "^27.0.0",
    "babel-eslint": "^10.1.0",
    "eslint": "^8.57.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-vue": "^9.23.0",
    "eslint-plugin-vuetify": "^1.0.1",
    "prettier": "^3.2.5",
    "sass": "< 1.33.0",
    "sass-loader": "^14.1.1",
    "vue-cli-plugin-vuetify": "^2.5.8",
    "vue-template-compiler": "^2.7.16",
    "vuetify-loader": "^1.9.2"
  }
